{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Warning",
      "Microsoft.AspNetCore.SignalR.Internal": "None",
      "Microsoft.EntityFrameworkCore": "Information",
      "LancacheManager": "Debug"
    }
  },
  "Environment": "Development",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=data/db/LancacheManager.db"
  },
  "LanCache": {
    "CachePath": "cache",
    "LogPath": "logs",
    "AutoDiscoverDatasources": true
  },
  "Security": {
    "EnableAuthentication": true,
    "RequireAuthForMetrics": false
  },
  "Optimizations": {
    "EnableGarbageCollectionManagement": true
  }
}
