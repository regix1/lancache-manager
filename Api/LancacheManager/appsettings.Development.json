{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "LancacheManager": "Debug"
    }
  },
  "Environment": "Development",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=data/lancache.db"
  },
  "LanCache": {
    "CachePath": "cache",
    "StartFromEndOfLog": true
  },
  "Security": {
    "ApiKeyPath": "data/api_key.txt",
    "DevicesPath": "data/devices",
    "EnableAuthentication": false,
    "RequireAuthForMetrics": false
  }
}