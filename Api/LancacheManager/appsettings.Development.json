{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "LancacheManager": "Information"
    }
  },
  "Security": {
    "ApiKeyPath": "api_key.txt",
    "DevicesPath": "devices"
  },
  "LanCache": {
    "LogPath": "access.log",
    "CachePath": "cache",
    "StartFromEndOfLog": true
  }
}