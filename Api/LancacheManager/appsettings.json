{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "LancacheManager.Services.LogWatcherService": "Information",
      "LancacheManager.Services.LogParserService": "Warning",
      "LancacheManager.Services.DatabaseService": "Information",
      "LancacheManager.Services.DownloadCleanupService": "Information",
      "LancacheManager.Services.CacheManagementService": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/data/lancache.db"
  },
  "LanCache": {
    "LogPath": "/logs/access.log",
    "CachePath": "/cache",
    "StartFromEndOfLog": true,
    "ProcessHistoricalLogs": false
  }
}