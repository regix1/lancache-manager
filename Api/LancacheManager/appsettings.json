{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error",
      "Microsoft.EntityFrameworkCore.Database.Command": "None",
      "Microsoft.EntityFrameworkCore.Database": "None",
      "Microsoft.Extensions.Hosting": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "LancacheManager": "Information"
    }
  },
  "AllowedHosts": "*",
  "Environment": "Production",
  "Server": {
    "ExternalUrl": "",
    "ExternalHost": "",
    "ExternalPort": ""
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=data/lancache.db"
  },
  "LanCache": {
    "CachePath": "cache",
    "StartFromEndOfLog": true
  },
  "CacheClear": {
    "DeleteMode": "preserve"
  },
  "Security": {
    "EnableAuthentication": true,
    "RequireAuthForMetrics": false,
    "MaxAdminDevices": 3,
    "GuestSessionDurationHours": 6
  },
  "Optimizations": {
    "EnableGarbageCollectionManagement": false
  },
  "NginxLogRotation": {
    "Enabled": true,
    "ContainerName": ""
  }
}