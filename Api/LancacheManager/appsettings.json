{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  "Windows": {
    "DataDirectory": "C:\\ProgramData\\LancacheManager",
    "LanCache": {
      "LogPath": "C:\\ProgramData\\LancacheManager\\logs\\access.log",
      "CachePath": "C:\\ProgramData\\LancacheManager\\cache",
      "StartFromEndOfLog": true
    }
  },
  
  "Linux": {
    "DataDirectory": "/data",
    "LanCache": {
      "LogPath": "/logs/access.log",
      "CachePath": "/cache",
      "StartFromEndOfLog": true
    }
  },
  
  "DataDirectory": null,
  "LanCache": {
    "LogPath": null,
    "CachePath": null,
    "StartFromEndOfLog": true
  }
}