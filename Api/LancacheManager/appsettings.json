{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error",
      "Microsoft.EntityFrameworkCore.Database.Command": "None",
      "Microsoft.EntityFrameworkCore.Database": "None",
      "Microsoft.Extensions.Hosting": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "LancacheManager": "Information"
    }
  },
  "AllowedHosts": "*",
  "Environment": "Production",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=data/LancacheManager.db"
  },
  "LanCache": {
    "CachePath": "cache"
  },
  "CacheClear": {
    "DeleteMode": "preserve"
  },
  "Security": {
    "EnableAuthentication": true,
    "RequireAuthForMetrics": false,
    "MaxAdminDevices": 3,
    "GuestSessionDurationHours": 6,
    "AllowedOrigins": "",
    "DeviceKeySecret": "",
    "AllowedBrowsePaths": "",
    "ProtectSwagger": false
  },
  "Optimizations": {
    "EnableGarbageCollectionManagement": false
  },
  "NginxLogRotation": {
    "Enabled": true,
    "ContainerName": ""
  },
  "ApiOptions": {
    "MaxClientsPerRequest": 1000,
    "DefaultClientsLimit": 100
  }
}